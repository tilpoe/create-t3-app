{
  "name": "template",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun generate --build && next build",
    "check": "bun generate --build && next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "concurrently \"next dev --turbo\" \"bun generate\"",
    "dev:no-turbo": "concurrently \"next dev\" \"bun generate\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "generate": "bun run .gen/run.ts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "bun generate --build && tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.469.0",
    "next": "^15.1.2",
    "next-safe-action": "^7.10.0",
    "nuqs": "^2.2.3",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "sonner": "^1.7.0",
    "@tanstack/react-form": "^0.41.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/bun": "^1.1.14",
    "@types/eslint": "^8.56.10",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@babel/generator": "^7.26.3",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "chokidar": "^4.0.1",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "ts-morph": "^25.0.0",
    "typescript": "^5.5.3",
    "use-debounce": "^10.0.4"
  },
  "overrides": {
    "react-is": "^19.0.0"
  }
}
